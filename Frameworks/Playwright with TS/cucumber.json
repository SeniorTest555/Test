{
    "default":{
        "tags":"@test",
        "formatOptions": {
         "snippetInterface": "async-await"
        },
        "paths":[
            "src/test/features/"
            
        ],
        "publishQuiet": true,
        "DryRun":true,
        "require":[
            "src/test/steps/*.steps.ts",
            "src/hooks/hook.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json"
        ]
    }
}